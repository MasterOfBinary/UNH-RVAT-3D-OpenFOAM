FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Do a synchronisation of coupled points after creation of any patches.
// Note: this does not work with points that are on multiple coupled patches
//       with transformations (i.e. cyclics).
pointSync false;

// Patches to create.
patches
(
    {
        //- Master side patch
        name            AMI1;
        patchInfo
        {
            type            cyclicAMI;
            matchTolerance  1e-4; // Was 0.0001
            neighbourPatch  AMI2;
            transform       noOrdering;
        }
        constructFrom patches;
        patches (AMIsurface);
    }

    {
        //- Slave side patch
        name            AMI2;
        patchInfo
        {
            type            cyclicAMI;
            matchTolerance  1e-4; // Was 0.0001
            neighbourPatch  AMI1;
            transform       noOrdering;
        }
        constructFrom patches;
        patches (AMIsurface_slave);
    }

    {
        name inlet;

        patchInfo
        {
            type            patch;
        }

        constructFrom patches;

        patches (all_24);
    }
    {
        name outlet;

        patchInfo
        {
            type            patch;
        }

        constructFrom patches;

        patches (all_29);
    }

    {
        name walls;

        patchInfo
        {
            type            wall;
        }

        constructFrom patches;

        patches (all_25 all_27);
    }

    {
        name bottom;

        patchInfo
        {
            type            wall;
        }

        constructFrom patches;

        patches (all_28);
    }

    {
        name top;

        patchInfo
        {
            type            patch;
        }

        constructFrom patches;

        patches (all_26);
    }

    {
        name shaft;

        patchInfo
        {
            type            wall;
        }

        constructFrom patches;

        patches (all_0
                 all_1
                 all_22
                 all_23);
    }

    {
        name struts;

        patchInfo
        {
            type            wall;
        }

        constructFrom patches;

        patches (all_2
                 all_4
                 all_7
                 all_8
                 all_11
                 all_16
                 all_17
                 all_18
                 all_19
                 all_20
                 all_21);
    }

    {
        name blades;

        patchInfo
        {
            type            wall;
        }

        constructFrom patches;

        patches (all_3
                 all_5
                 all_6
                 all_9
                 all_10
                 all_12
                 all_13
                 all_14
                 all_15);
    }

);

