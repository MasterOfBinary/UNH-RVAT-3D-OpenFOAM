FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFormat vtk; 

interpolationScheme cellPoint; 

fields (U);

surfaces
(
    isoVorticity
    {
        type            isoSurface;
        isoField        magVorticity;
        isoValue        6;
        interpolate     true;
    }
);

